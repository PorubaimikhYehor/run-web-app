{
  "name": "run-web-app",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.yp.runner",
    "productName": "WebAppRunner",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "assets/mac-icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/windows-icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets"
    }
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "node-fetch": "^3.3.2",
    "yargs": "^17.7.2"
  }
}